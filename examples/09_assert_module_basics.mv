// std/assert module example
// Uses parser-safe assertion calls in std/assert.

include "std/assert";

fn main() -> int {
    assert_true(1, 0);
    assert_false(0, 0);
    assert_eq(10, 10, 0);
    assert_ne(10, 20, 0);
    assert(1, 0);
    assert_null(0, 0);
    assert_not_null(1, 0);
    return 0;
}
